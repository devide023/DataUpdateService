<?xml version="1.0" encoding="utf-8" ?>
<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.0">
<processing-directives>
    <overwrite-existing-data>true</overwrite-existing-data>
  </processing-directives>
  <schedule>
    <job>
      <name>GkDataSync</name>
      <group>GkDataSyncGroup</group>
      <description>爬取电影数据</description>
      <job-type>DataUpdateService.Jobs.GkDataSync,DataUpdateService</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>GkDataSyncTrigger</name>
        <group>Update</group>
        <job-name>GkDataSync</job-name>
        <job-group>GkDataSyncGroup</job-group>
        <cron-expression>0 0 0,4,18 * * ?</cron-expression>
      </cron>
    </trigger>
    <job>
      <name>WorkTask</name>
      <group>GkDataSyncGroup</group>
      <description>爬取威客网数据</description>
      <job-type>DataUpdateService.Jobs.WorkTask,DataUpdateService</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>WorkTaskTrigger</name>
        <group>Update</group>
        <job-name>WorkTask</job-name>
        <job-group>GkDataSyncGroup</job-group>
        <cron-expression>0 0 3,9,12,15,18,20,23 * * ?</cron-expression>
      </cron>
    </trigger>
    <job>
      <name>RRKFTask</name>
      <group>GkDataSyncGroup</group>
      <description>人人开发网数据</description>
      <job-type>DataUpdateService.Jobs.RRKFJob,DataUpdateService</job-type>
      <durable>true</durable>
      <recover>false</recover>
    </job>
    <trigger>
      <cron>
        <name>RRKFTaskTrigger</name>
        <group>Update</group>
        <job-name>RRKFTask</job-name>
        <job-group>GkDataSyncGroup</job-group>
        <cron-expression>0 0 9,12,21 * * ?</cron-expression>
      </cron>
    </trigger>
  </schedule>
</job-scheduling-data>